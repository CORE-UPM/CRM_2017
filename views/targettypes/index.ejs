

<h1> 
    Tipos de Objetivo: 

    <a href="/targettypes/new"><img class="icon" src="/images/add.png"></a>
</h1>

<table>
    <tr>
        <th>
            Nombre
        </th>
        <th>
            
        </th>
    </tr> 

    <% for (var i in targettypes) { %> 
        <% var targettype = targettypes[i]; %>      

        <tr>
            <td>
                <%= targettype.name %>
            </td>
            <td>
                <a href="/targettypes/<%= targettype.id %>"><img class="icon" src="/images/ver.png"></a>

                <a href="/targettypes/<%= targettype.id %>/edit"><img class="icon" src="/images/edit.png"></a>

                <a href="/targettypes/<%= targettype.id %>?_method=DELETE" 
                   onClick="return confirm('Â¿Seguro que quiere borrar el tipo de objetivo: <%= targettype.name %>?');"><img class="icon" src="/images/basura.png"></a>
            </td>
         </tr> 
    <% } %>

</table>

