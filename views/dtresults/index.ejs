

<h2> 

    Resultados: 

    <a href="/dtypes/<%= dtype.id %>/dtresults/new">
        <button type="button">Crear nuevo resultado</button>
    </a>

</h2>


<ul>
    <% for (var i in dtresults) { %> 
        <% var dtresult = dtresults[i]; %>      

        <li>
            <b> <%= dtresult.code %> - <%= dtresult.title %> </b>

            <a href="/dtypes/<%= dtype.id %>/dtresults/<%= dtresult.id %>/edit"><button>editar</button></a>

            <a href="/dtypes/<%= dtype.id %>/dtresults/<%= dtresult.id %>?_method=DELETE" 
                onClick="return confirm('Borrar: <%= dtresult.code %>');">
                <button>borrar</button>
            </a>

            <p>
               <%- escapeText(dtresult.description) %>
            </p>

            <%  var dtroptions = dtresult.DTROptions %>

            <%- include ../dtroptions/index.ejs %>

        </li> 
    <% } %>
</ul>


