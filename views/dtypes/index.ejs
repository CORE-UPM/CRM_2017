

<h2> Tipos de Diagnósticos </h2>

<table>
  <% for (var i in dtypes) { %> 
    <% var dtype = dtypes[i]; %>      

     <tr>
        <td>
           <a href="/dtypes/<%= dtype.id %>"> <%= dtype.code %> - <%= dtype.title %> </a>
        </td>

        <td>
            <a href="/dtypes/<%= dtype.id %>/edit"><button>editar</button></a>
            <a href="/dtypes/<%= dtype.id %>?_method=DELETE" 
               onClick="return confirm('Borrar: <%= dtype.code %>');">
               <button>borrar</button>
            </a>
        </td>
     </tr> 
  <% } %>
</table>


<a href="/dtypes/new"><button type="button">Crear nuevo tipo de diagnóstico</button></a>
